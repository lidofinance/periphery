{
  "parser": "@typescript-eslint/parser",
  "extends": ["plugin:@typescript-eslint/recommended", "prettier"],
  "parserOptions": { "ecmaVersion": 2022, "sourceType": "module" },
  "rules": {
    "@typescript-eslint/no-explicit-any": ["warn"]
  },
  "overrides": [
    {
      "files": ["./scripts/{**/,}*.js", "./test/{**/,}*.js"],
      "env": {
        "mocha": true
      }
    }
  ]
}
